{
    "name": "wilhelmsempre/vehicle-v2.0-client",
    "description": "Vehicle System. Vehicle System is system used to support vehicles. This is client for communicating with API.",
    "license": "LICENSED",
    "authors": [
        {
            "name": "Rafał Głuszak",
            "email": "rafal.gluszak@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^6.4",
        "knplabs/knp-paginator-bundle": "^5.0",
        "knplabs/knp-snappy-bundle": "^1.6",
        "liip/imagine-bundle": "^2.2",
        "sensio/framework-extra-bundle": "^5.5",
        "sensiolabs/security-checker": "^6.0",
        "symfony/console": "4.4.*",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "4.4.*",
        "symfony/filesystem": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/yaml": "4.4.*",
        "vich/uploader-bundle": "^1.12",
        "ext-json": "*"
    },
    "require-dev": {
        "easycorp/easy-log-handler": "^1.0.7",
        "phpunit/phpunit": "^8",
        "roave/security-advisories": "dev-master",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.14",
        "symfony/monolog-bundle": "^3.0",
        "symfony/profiler-pack": "*",
        "symfony/var-dumper": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
